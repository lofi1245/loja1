<div class="container">



<div class="card-footer">
<p id="notice"><%= notice %></p>
<h2>Veículos</h2>

<table>

  <thead>
  
    <tr>
    
      <th>Modelo</th>
      <th>Chassi</th>
      <th>Preco</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  
  <tbody>
    
    <% @vehicles.each do |vehicle| %>
      
      <tr>
      
        <td><%= vehicle.modelo %></td>
        <td><%= vehicle.chassi %></td>
        <td><%= vehicle.preco %></td>
        <td><%= link_to 'Show', vehicle %></td>
        <td><%= link_to 'Edit', edit_vehicle_path(vehicle) %></td>
        <td><%= link_to 'Destroy', vehicle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        
      </tr>
    <% end %>
  </tbody>
  
</table>

<br>

<%= link_to 'adicionar veiculo', new_vehicle_path %>
<%=link_to "sair da sessão",destroy_user_session_path,method: :delete%>
<%= link_to 'categorias', "/categories" %>
<%= link_to "Editar usuario", edit_user_registration_path %>
